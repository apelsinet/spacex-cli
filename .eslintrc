{
    "extends": "eslint:recommended",
        "parser": "babel-eslint",
        "parserOptions": {
            "ecmaVersion": 6,
            "ecmaFeatures": {
                "jsx": true,
                "experimentalObjectRestSpread": true
            },
            "sourceType": "module"
        },
        "globals": {
            "Promise": true,
            "$": true
        },
        "env": {
            "browser": true,
            "node": true,
            "jest": true
        },
        "plugins": [
            "react",
            "class-property"
        ],
        "rules": {
            "no-console": 0,
            "new-cap": 0,
            "strict": 0,
            "no-underscore-dangle": 0,
            "no-use-before-define": 0,
            "eol-last": 0,
            "quotes": [2, "single", "avoid-escape"],
            "jsx-quotes": 1,
            "react/jsx-no-undef": 1,
            "react/jsx-uses-react": 1,
            "react/jsx-uses-vars": 1,
            "react/prop-types": 1,
            "semi": 2,
            "class-property/class-property-semicolon": 2
        }
}
